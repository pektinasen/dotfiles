alias a2restart='sudo service apache2 restart'

# DOCKER
alias doco="docker-compose"
alias dcdr="docker-compose exec --user 82 php drush"
alias dcdd="docker-compose exec --user 82 php drupal"

alias ll="ls -alh"

# ubuntu
alias sagi="sudo apt install"
alias sagu="sudo apt update"
